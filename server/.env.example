# OpenSky Network API Credentials
OPENSKY_USER=your-opensky-username
OPENSKY_PASS=your-opensky-password

# Server Configuration
PORT=3005
NODE_ENV=production

# PostgreSQL Database
POSTGRES_URL=postgresql://example:example@db:5432/fly_overhead

# AWS Configuration
AWS_REGION=us-west-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# N2YO API Key for Satellite Data
N2YO_API_KEY=your-n2yo-api-key

# Aerodatabox API (used for route enrichment when callsign is missing)
AERODATABOX_API_KEY=your-aerodatabox-api-key
# Optional: Set a conservative daily budget to avoid exceeding monthly quota (defaults to 600)
AERODATABOX_DAILY_BUDGET=600

# Frontend API URL (used by React app)
REACT_APP_API_URL=http://localhost:3005
