{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["error", 2],
      "linebreak-style": ["error", "unix"],
      "no-await-in-loop": "off",
      "no-promise-executor-return": "off",
      "import/no-extraneous-dependencies": "off",
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"],
      "no-console": "off"
    }
  }